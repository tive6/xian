/* nav */
.nav
  width 100%
  padding 5px 0
  position fixed
  left 0
  bottom 0
  background-color #fff
  border-top 1px solid #ebebeb
.nav-list
  display block
  width 20%
  float left
  text-align center
  .nav-list-icon
    background-size 100%
.nav-list3
  position relative
.nav-list-icon
  width 25px
  height 25px
  margin 0 auto
.nav-list-icon3-inner
  width 50px
  height 50px
  background url(./../../assets/img/home/nav-icon-03.png) no-repeat center center
  background-size 100%
$navUrls = 1 2 4 5
for urls in $navUrls
  .nav-list-icon{urls}
    background url('./../../assets/img/home/nav-icon-0'urls'.png') no-repeat center center
.nav-list-active
  .nav-list-icon1
    background url(./../../assets/img/home/nav-icon-01-active.png) no-repeat center center
  .nav-list-icon2
    background url(./../../assets/img/home/nav-icon-02-active.png) no-repeat center center
  .nav-list-icon4
    background url(./../../assets/img/home/nav-icon-04.png) no-repeat center center
  .nav-list-icon5
    background url(./../../assets/img/home/nav-icon-05-active.png) no-repeat center center
  .nav-list-icon
    background-size 100%
