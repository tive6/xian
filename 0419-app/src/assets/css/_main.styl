/* index banner */
.index-banner
  position relative
.index-banner-msg
  @extend .rt-msg
.swiper-slide
  display block
  width 100%
  height 200px
  background-color $bgGrayBd
  background-position center center
  background-size 100%
.swiper-container-horizontal
  &>.swiper-pagination-bullets
    bottom 30px
.swiper-pagination-bullet
  background-color $bgFf
.swiper-pagination-bullet-active
  background-color $colorBlue
/*.swiper-container-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet
  margin 0 8px*/

/* index */
.index-f
  margin 0 15px
.index-f1
  height 120px
  background-color $bgFf
  border-radius $borderR8
  box-shadow 0 0 3px #fbb
  //background url(./../img/home/home-bg-f2.png) no-repeat
  //background-size cover
  padding 20px 0 0
  margin-top -22px
  position relative
  z-index 10
  .index-f1-list
    width 33.33%
    float left
  .index-f1-img
    display block
    margin 0 auto
    width 50px
  .index-f1-text
    color $color6
    text-align center
    margin-top 19px
.index-f2
  padding 0 20px 16px
  margin-top 10px
  background-color $bgFf
  border-radius $borderR8
  text-align center
  box-shadow 0 0 3px #fbb
.index-f2-title
  font-size $font16
  padding 20px 0
  border-bottom 1px solid $colorBor
.index-f2-num
  font-size 52px
  color $colorRed
  margin-top 19px
.index-f2-percent
  font-size 15px
.index-f2-desc
  color $color9
  margin 15px 0 28px
.index-f2-spec-list
  display inline-block
.index-f2-spec-icon
  display inline-block
  width 15px
  vertical-align middle
.index-f2-spec-inner
  font-size 12px
  color $color9
.index-f2-spec-list2
  margin 0 9px
.index-f2-btn
  display block
  margin 18px 0 0
  font-size 16px
  color $bgFf
  background-color $colorRed
  line-height 50px
  border-radius 25px
.index-f3
  padding 9px 0
  text-align center
  font-size 12px
  color $color9

/* touzi */
.touzi
  padding 10px 15px 70px
.touzi-list
